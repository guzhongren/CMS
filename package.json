{
  "name": "CMS",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "homepage": "https://github.com/guzhongren/CMS",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "live": "live-server ./docs",
    "deploy": "gh-pages -d docs",
    "lint": "yarn _typeCheck && yarn _tsLint",
    "testCoverage": "jest --coverage",
    "testCoverageLive": "jest --coverage && live-server ./coverage/lcov-report ",
    "test": "jest",
    "tdd": "gulp",
    "_typeCheck": "tsc --project tsconfig.json --noEmit",
    "_tsLint": "tslint -p ./tsconfig.json ./src/**/*.{ts,tsx}",
    "release": "standard-version --release-as patch",
    "minorRelease": "standard-version --release-as minor",
    "majorRelease": "standard-version --release-as major",
    "buildImage": "docker build -t cms . && docker tag cms guzhongren/cms:latest",
    "pushImage": "docker push guzhongren/cms:latest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "pre-push": "yarn lint"
    }
  },
  "standard": {
    "globals": [
      "console",
      "Error",
      "config",
      "$",
      "antd",
      "woff"
    ],
    "properties": false,
    "parser": "babel-eslint",
    "ignore": [
      "./public/dist/",
      "./build/",
      "./styleguide/"
    ]
  },
  "browserslist": [
    "defaults",
    "last 5 version",
    "> 1%",
    "not ie <= 9"
  ],
  "jest": {
    "collectCoverage": false,
    "collectCoverageFrom": [
      "**/src/**/*.{ts,tsx}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lib/"
    ],
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "setupTestFrameworkScriptFile": "./setupTests.ts",
    "testRegex": "(/test/.*|\\.(test|spec))\\.(ts|tsx|jsx|js)$",
    "moduleNameMapper": {
      "\\.(less|scss|css|jpg|png|svg)$": "jest-css-modules",
      "^@config(.*)$": "<rootDir>/src/config$1",
      "^@components(.*)$": "<rootDir>/src/components$1",
      "^@pages(.*)$": "<rootDir>/src/pages$1",
      "^@utils(.*)$": "<rootDir>/src/utils$1",
      "^@layouts(.*)$": "<rootDir>/src/layouts$1",
      "^@api(.*)$": "<rootDir>/src/api$1",
      "^@assets(.*)$": "<rootDir>/src/assets$1",
      "^@theme(.*)$": "<rootDir>/src/theme$1"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "babelConfig": false
      }
    }
  },
  "dependencies": {
    "antd": "^3.10.3",
    "axios": "^0.18.0",
    "less": "^3.8.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-draggable": "^3.0.5",
    "react-icons": "^3.0.1",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "@types/react-router-dom": "^4.2.7",
    "@babel/polyfill": "^7.0.0",
    "@types/classnames": "^2.2.6",
    "@types/enzyme": "^3.1.14",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.5",
    "@types/node": "^10.3.2",
    "@types/react": "^16.3.17",
    "@types/react-dom": "^16.0.6",
    "autoprefixer": "^9.1.5",
    "awesome-typescript-loader": "^5.2.0",
    "axios-mock-adapter": "^1.15.0",
    "axios-progress-bar": "^1.2.0",
    "browserslist": "^4.2.0",
    "classnames": "^2.2.6",
    "clean-webpack-plugin": "^1.0.0",
    "console-polyfill": "^0.3.0",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "css-hot-loader": "^1.3.9",
    "css-loader": "^2.0.0",
    "cssnano": "^4.0.1",
    "del-cli": "^1.1.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "file-loader": "^3.0.1",
    "gh-pages": "^2.0.1",
    "happypack": "^5.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.0.0-rc.9",
    "jest": "^23.6.0",
    "jest-canvas-mock": "^1.1.0",
    "jest-css-modules": "^1.1.0",
    "less-loader": "^4.1.0",
    "live-server": "^1.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "moment": "^2.22.2",
    "postcss-calc": "^7.0.1",
    "postcss-color-rgba-fallback": "^3.0.0",
    "postcss-easy-import": "^3.0.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-flexibility": "^2.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-opacity": "^5.0.0",
    "progress-bar-webpack-plugin": "^1.11.0",
    "resolve-url-loader": "^3.0.0",
    "source-map-loader": "^0.2.3",
    "standard": "^12.0.1",
    "standard-version": "^4.4.0",
    "style-loader": "^0.23.1",
    "styled-jsx": "^3.1.0",
    "ts-import-plugin": "^1.5.4",
    "ts-jest": "^23.10.4",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.0.1",
    "url-loader": "^1.0.1",
    "webpack": "^4.11.1",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-cli": "^3.0.3",
    "webpack-dev-server": "^3.1.4",
    "webpack-parallel-uglify-plugin": "^1.1.0",
    "workbox-webpack-plugin": "^3.6.2"
  }
}
